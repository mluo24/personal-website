{
  "name": "personal-website",
  "version": "1.0.0",
  "private": true,
  "description": "Miranda Luo's Personal Website",
  "author": "mluo24",
  "keywords": [
    "gatsby"
  ],
  "scripts": {
    "develop": "gatsby develop",
    "start": "gatsby develop",
    "build": "gatsby build",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "format": "prettier --write \"**/*.+(js|jsx|ts|tsx|json|css|md)\"",
    "deploy": "gatsby build --prefix-paths && gh-pages -d public -r https://${GITHUB_TOKEN}@github.com/mluo24/mluo24.github.io.git"
  },
  "dependencies": {
    "@emotion/react": "^11.7.1",
    "@emotion/server": "^11.4.0",
    "@emotion/styled": "^11.6.0",
    "@mantine/core": "^4.2.12",
    "@mantine/hooks": "^4.2.12",
    "@mdx-js/mdx": "^1.6.22",
    "@mdx-js/react": "^1.6.22",
    "@mui/icons-material": "^5.2.5",
    "@mui/lab": "^5.0.0-alpha.61",
    "@mui/material": "^5.2.5",
    "@mui/system": "^5.8.7",
    "@mui/types": "^7.1.0",
    "@types/mdx-js__react": "^1.5.5",
    "@types/react": "^17.0.37",
    "@types/react-dom": "^17.0.11",
    "@types/smooth-scroll": "^16.1.1",
    "gatsby": "^4.4.0",
    "gatsby-plugin-google-gtag": "^4.18.0",
    "gatsby-plugin-graphql-codegen": "^3.1.0",
    "gatsby-plugin-image": "^2.5.1",
    "gatsby-plugin-mantine": "^4.0.0",
    "gatsby-plugin-mdx": "^3.4.0",
    "gatsby-plugin-react-helmet": "^5.4.0",
    "gatsby-plugin-sharp": "^4.5.1",
    "gatsby-source-filesystem": "^4.4.0",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-helmet": "^6.1.0",
    "smooth-scroll": "^16.1.3",
    "tabler-icons-react": "^1.53.0"
  },
  "devDependencies": {
    "@emotion/cache": "^11.7.1",
    "@types/react-helmet": "^6.1.5",
    "@typescript-eslint/eslint-plugin": "^5.8.0",
    "@typescript-eslint/parser": "^5.8.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-config-react-app": "^7.0.0",
    "eslint-plugin-prettier": "^4.0.0",
    "gh-pages": "^3.2.3",
    "husky": "^7.0.4",
    "lint-staged": "^12.1.3",
    "prettier": "^2.5.1",
    "prop-types": "^15.8.0",
    "typescript": "^4.5.4"
  },
  "resolutions": {
    "@types/react": "17.0.37",
    "@types/react-dom": "17.0.11"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn run lint && yarn run format"
    }
  },
  "lint-staged": {
    "*.+(js|jsx|ts|tsx)": "eslint --fix",
    "*.+(js|jsx|ts|tsx|json|css|md)": "prettier --write"
  }
}
